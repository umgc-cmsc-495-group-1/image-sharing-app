rules_version = "2";
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow only authenticated content owners access
  	match /users/{documents=**} {
        allow read, create, write, update, delete: if true;
    }
    match /posts/{documents=**} {
      allow read, create, write, update, delete: if true;
    }
  }
}
